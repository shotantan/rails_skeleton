<p id="notice"><%= notice %></p>

<h1>Listing Mailenables</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @mailenables.each do |mailenable| %>
      <tr>
        <td><%= mailenable.name %></td>
        <td><%= link_to 'Show', mailenable %></td>
        <td><%= link_to 'Edit', edit_mailenable_path(mailenable) %></td>
        <td><%= link_to 'Destroy', mailenable, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mailenable', new_mailenable_path %>
